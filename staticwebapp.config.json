{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/Icons/*", "/api/*", "/*.png", "/*.svg", "/*.ico"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  }
}
